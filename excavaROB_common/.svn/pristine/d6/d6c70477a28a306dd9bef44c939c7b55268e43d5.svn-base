  <node name="gazebo" pkg="gazebo" type="gazebo" args="$(find gazebo)/gazebo/share/gazebo/worlds/terrain.world" respawn="false" output="screen"/>
<!-- $(find gazebo)/gazebo/share/gazebo/worlds/terrain.world REVISAR -->
</launch>
